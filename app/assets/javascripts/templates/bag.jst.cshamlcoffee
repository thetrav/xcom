.bag{"data-id": @bag.id }
  .label{"data-toggle":"collapse", "data-target":"#bag##{@bag.id}"}
    .icons
      .used-capacity= @bag.get("used_capacity")
      .capacity= @bag.get("capacity")
    =@bag.get("name")
  .contents{"id":"bag#{@bag.id}"}
    - for item in @bag.get("items")
      != JST["templates/item"].call(item: new Page.Models.Item(item))