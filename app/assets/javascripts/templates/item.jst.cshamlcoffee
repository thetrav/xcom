.item{"data-id": @item.get("id") }
  .item-label
    .icons
      .item-space= @item.get("space")
      .item-weight= @item.get("total_weight")
    = @item.get("name")
  - for slot in @item.get("slot_children")
    != JST["templates/slot"].call(slot: new Page.Models.Slot(slot))

  -bag = @item.get("bag_child")
  != if bag then JST["templates/bag"].call(bag: new Page.Models.Bag(bag)) else ""